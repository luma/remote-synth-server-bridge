{
  "name": "remote-synth-server-bridge",
  "version": "0.0.1",
  "description": "A native bridge based on WebRTC for remote sync server",
  "main": "lib/index.js",
  "scripts": {
    "build": "node-gyp --debug rebuild",
    "clean": "node-gyp clean",
    "test": "node examples/test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jraoult/remote-synth-server-bridge.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jraoult/remote-synth-server-bridge/issues"
  },
  "homepage": "https://github.com/jraoult/remote-synth-server-bridge",
  "gypfile": true,
  "dependencies": {
    "commander": "2.8.1",
    "segfault-handler": "~0.2.4"
  },
  "jshintConfig": {
    "globals": {
      "require": true,
      "console": true,
      "process": true
    },
    "globalstrict": true,
    "browser": true,
    "esnext": true,
    "strict": false,
    "camelcase": true,
    "eqeqeq": true,
    "eqnull": true,
    "expr": true,
    "indent": 2,
    "latedef": true,
    "maxlen": 100,
    "newcap": true,
    "quotmark": "single",
    "smarttabs": true,
    "trailing": true,
    "undef": true,
    "unused": true
  }
}